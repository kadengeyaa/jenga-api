{
  "name": "jenga-api",
  "version": "1.0.2",
  "description": "APIs to integrate Equity Jenga Payments",
  "exports": "./lib/index.mjs",
  "main": "lib/index.mjs",
  "types": "lib/index.d.mts",
  "engines": {
    "node": ">=14.16"
  },
  "author": "kadengeyaa <github.com/kadengeyaa>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "lint": "eslint --ignore-path .eslintignore --ext .mts src/ --fix",
    "format": "prettier --write '**/*.mts'",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadengeyaa/jenga-api.git"
  },
  "keywords": [
    "Jenga",
    "Equity Bank",
    "Payments",
    "Payment gateway"
  ],
  "bugs": {
    "url": "https://github.com/kadengeyaa/jenga-api/issues"
  },
  "homepage": "https://github.com/kadengeyaa/jenga-api#readme",
  "files": [
    "build/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.11.9",
    "@types/prettyjson": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettyjson": "^1.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.8.2"
  }
}
